{
  "kind": "collectionType",
  "collectionName": "ctas",
  "info": {
    "singularName": "cta",
    "pluralName": "ctas",
    "displayName": "CTA",
    "description": "Call-to-action section variants that can be used on different pages"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "titleLine1",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "titleLine1": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "titleLine2": {
      "type": "string",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "subtext": {
      "type": "text",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ctaButtonText": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ctaButtonHref": {
      "type": "string",
      "required": false,
      "default": "/contact",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}

